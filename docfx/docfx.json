{
  "metadata": [
    {
      "src": [
        {
          "files": ["CodeGlyphX/CodeGlyphX.csproj"],
          "src": ".."
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "allowCompilationErrors": false,
      "properties": {
        "TargetFramework": "net8.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": ["api/**.yml", "api/index.md"]
      },
      {
        "files": ["articles/**.md", "articles/**/toc.yml", "toc.yml", "*.md"]
      }
    ],
    "resource": [
      {
        "files": ["images/**"]
      }
    ],
    "output": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": ["default", "modern"],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "CodeGlyphX",
      "_appName": "CodeGlyphX",
      "_appFooter": "© Evotec Services sp. z o.o. | Created by Przemysław Kłys",
      "_enableSearch": true,
      "_gitContribute": {
        "repo": "https://github.com/EvotecIT/CodeGlyphX",
        "branch": "master"
      }
    }
  }
}
